  "image": "ghcr.io/krystianpietron/i2c-addon",
  "privileged": ["SYS_RAWIO"],
  "init": false,
  "build": {
    "args": {
      "BUILD_FROM": "homeassistant/{arch}-base-python:3.12"
    }
  }
